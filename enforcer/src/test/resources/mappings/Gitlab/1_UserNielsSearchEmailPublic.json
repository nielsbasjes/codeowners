{
  "priority": 1,
  "request": {
    "method": "GET",
    "urlPath": "/api/v4/users",
    "headers": {
      "PRIVATE-TOKEN": {
        "equalTo": "gltst-validtoken"
      }
    },
    "queryParameters": {
      "search": {
        "equalTo": "public@example.nl"
      }
    }
  },
  "response": {
    "transformers": [
      "response-template"
    ],
    "status": 200,
    "headers": {
      "Content-Type": "application/json;charset=UTF-8",
      "X-Next-Page": "",
      "X-Page": "1",
      "X-Per-Page": "20",
      "X-Prev-Page": "",
      "X-Total": "4",
      "X-Total-Pages": "1"
    },
    "bodyFileName": "gitlab/NielsUser.json"
  }
}
